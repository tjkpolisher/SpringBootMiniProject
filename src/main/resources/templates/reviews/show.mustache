{{>layouts/header}}
<table class='table'>
    <thread>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Title</th>
            <th scope="col">Content</th>
        </tr>
    </thread>
    <tbody>
        {{#reviews}}
        <tr>
            <th>{{id}}</th>
            <th>{{title}}</th>
            <th>{{content}}</th>
        </tr>
        {{/reviews}}
    </tbody>
</table>

<a href="/reviews">메인</a>
<a href="/reviews/{{reviews.id}}/edit" class="btn btn-primary">수정</a>
<a href="/reviews/{{reviews.id}}/delete" class="btn btn-danger">삭제</a>
{{>layouts/footer}}